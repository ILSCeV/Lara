{
  "authors": [
    {
      "email": "4d44h@protonmail.com",
      "name": "Maxim Drachinskiy",
      "role": "Project Manager"
    }
  ],
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "Lara\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests",
      "database"
    ],
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "description": "A calender with personnel scheduling for club management, built on top of Laravel 8.",
  "keywords": [
    "calendar",
    "scheduling",
    "Lara",
    "ILSCeV"
  ],
  "license": "GPL-3.0",
  "name": "ilsc-ev/lara",
  "require": {
    "php": ">=7.3",
    "ext-json": "*",
    "bacon/bacon-qr-code": "^2.0",
    "barryvdh/laravel-ide-helper": "^2.6",
    "eluceo/ical": "^2.0.0",
    "erusev/parsedown": "^1.7",


    "johngrogg/ics-parser": "^3",
    "justinrainbow/json-schema": "^5.2",
    "laravel/framework": "^8",
    "laravel/legacy-factories": "^1.3",
    "laravel/tinker": "^2.0",
    "laravel/ui": "^3.0",
    "laravelcollective/html": "^6",
    "pragmarx/google2fa-laravel": "^2.0",
    "rap2hpoutre/laravel-log-viewer": "^2.0",
    "wol-soft/php-json-schema-model-generator-production": "^0.19.0"
  },
  "require-dev": {
    "nunomaduro/collision": "^8.0",
    "doctrine/dbal": "^3.0",
    "facade/ignition": "^2.5",
    "fakerphp/faker": "^1.14",
    "filp/whoops": "^2.7.3",
    "mockery/mockery": "~1.6.0",
    "orangehill/iseed": "dev-master",
    "phpunit/phpunit": "^9"
  },
  "scripts": {
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall"
    ],
    "post-root-package-install": [
      "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan ide-helper:generate",
      "php artisan ide-helper:meta"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ]
  },
  "type": "project",
  "minimum-stability": "dev",
  "prefer-stable": true
}
